        // Functions
        function swapDropdown(radioVar) {
            var varArray = eval(radioVar + "PointArray");
            var dropDownObject = eval("document.getData.station.options");
            dropDownObject.length = 0;
            for (i=0; i<varArray.length; i++)
                dropDownObject[i] = new Option(varArray[i][0], varArray[i][1]);
            dropDownObject[0].selected = true;
        }
        function swapProdDesc(curProd) {
            var varArray = eval("prodDescArray");
            var textAreaObject = eval("document.getData.prodDesc");
            var testVal = curProd;
            for (i=0; i<varArray.length; i++)
                if (testVal == varArray[i][1])
                    textAreaObject.value = varArray[i][0];
        }
        function swapDateDropdown() {
            var dropDownObject = eval("document.getData.date.options");
            for (i=0; i<document.getData.product.length;i++)
                if (document.getData.product[i].checked)
                    var curProd = document.getData.product[i].value;
            var curLoc = document.getData.station[document.getData.station.selectedIndex].value;
            var dateArray = eval("date" + curLoc + curProd + "Array");
            dropDownObject.length = 0;
            if (dateArray.length > 0 )
                for (j=0; j<dateArray.length; j++)
                    dropDownObject[j] = new Option(dateArray[j][0], dateArray[j][1]);
            else dropDownObject[0] = new Option("No Records Found","0");
            if (document.getData.recent[1].checked!=0)
                dropDownObject[0].selected = true;
        }
        function clearPulldown() {
            var pulldown_length = document.getData.date.length;
            for (x=0; x < pulldown_length; x++)
                document.getData.date.options[x].selected = false;
        }
        function disablePulldown() {
            if (document.getData.recent[0].checked!=0)
                document.getData.date.disabled=true;
            else document.getData.date.disabled=false;
        }

